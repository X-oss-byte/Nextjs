{
  "name": "create-next-app",
  "version": "13.4.17",
  "keywords": [
    "react",
    "next",
    "next.js"
  ],
  "description": "Create Next.js-powered React apps with one command",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/next.js",
    "directory": "packages/create-next-app"
  },
  "author": "Next.js Team <support@vercel.com>",
  "license": "MIT",
  "bin": {
    "create-next-app": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "prerelease": "node ../../scripts/rm.mjs dist",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "prepublishOnly": "cd ../../ && turbo run build",
    "build": "pnpm release",
    "lint-fix": "pnpm prettier -w --plugin prettier-plugin-tailwindcss 'templates/*-tw/{ts,js}/{app,pages}/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.8",
    "@types/ci-info": "2.0.0",
    "@types/cross-spawn": "6.0.6",
    "@types/node": "^20.2.5",
    "@types/prompts": "2.4.9",
    "@types/tar": "6.1.11",
    "@types/validate-npm-package-name": "4.0.2",
    "@vercel/ncc": "0.38.1",
    "async-retry": "1.3.3",
    "ci-info": "watson/ci-info#3e1488e98680f1f776785fe8708a157b7f00e568",
    "commander": "11.1.0",
    "conf": "12.0.0",
    "cross-spawn": "7.0.3",
    "fast-glob": "3.3.2",
    "got": "14.0.0",
    "picocolors": "1.0.0",
    "prettier-plugin-tailwindcss": "0.5.11",
    "prompts": "2.4.2",
    "tar": "6.2.0",
    "update-check": "1.5.4",
    "validate-npm-package-name": "5.0.0"
  },
  "engines": {
    "node": ">=16.8.0"
  }
}
